{
  "expo": {
    "name": "Location Based Visual Certification",
    "slug": "LoBaViCerty",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.13",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Certified.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.machynworks.lobavicerty",
      "versionCode": 13,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE", 
        "READ_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION"
      ]
    }
  }
}